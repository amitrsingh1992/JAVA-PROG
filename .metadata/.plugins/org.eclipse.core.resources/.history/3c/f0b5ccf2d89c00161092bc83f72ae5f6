import java.util.Scanner;
public class TwoDArray {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner n = new Scanner(System.in);
		int mRows = n.nextInt();
		int nCols = n.nextInt();
			int twoD[][]= new int[mRows][nCols];
			int i, j, k = 0;
			for(i=0; i<mRows; i++)
			for(j=0; j<nCols; j++) {
			twoD[i][j] = k;
			k++;
			}
			for(i=0; i<mRows; i++) {
			for(j=0; j<nCols; j++)
			System.out.print(twoD[i][j] + " ");
			System.out.println();
			}
			
		n.close();
		
	}

}
